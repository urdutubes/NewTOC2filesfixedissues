function displayItem() {
  var h = "";
  h += '<table width="100%"><tr>';
  h += '<td id="ds-id1"><form>Title Search : <input type="text" id="toc-search" onkeyup="searchPosts()" /></form></td>';
  h += '<td id="ds-id2" align="right"></td>';
  h += '</tr></table>';
  document.getElementById("toc-display").innerHTML = h;
}

/* ðŸ”§ loading() SAFE */
function loading() {
  var l = document.getElementById("toc-loading");
  if (l && l.parentNode) {
    l.parentNode.removeChild(l);
  }
}

handleSafari();
loading();
